{
  "name": "prisma-schema-user",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/prisma-schema-user/src",
  "projectType": "library",
  "tags": ["type:prisma-schema", "scope:user"],
  "// targets": "to see all targets run: nx show project prisma-schema-user --web",
  "targets": {
    "prisma": {
      "command": "prisma",
      "options": {
        "cwd": "libs/prisma-schema-user"
      }
    },
    "generate-types": {
      "command": "prisma generate",
      "options": {
        "cwd": "libs/prisma-schema-user"
      }
    },
    "db:push": {
      "command": "prisma db push",
      "options": {
        "cwd": "libs/prisma-schema-user"
      }
    },
    "migrate": {
      "command": "prisma migrate dev",
      "options": {
        "cwd": "libs/prisma-schema-user"
      }
    }
  }
}
